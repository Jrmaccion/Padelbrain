{
  "cli": {
    "version": ">=13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "resourceClass": "default" },
      "android": { "resourceClass": "default" }
    },
    "preview": {
      "distribution": "internal",
      "ios": { "resourceClass": "default" },
      "android": { "resourceClass": "default" }
    },
    "production": {
      "ios": { "resourceClass": "default" },
      "android": { "resourceClass": "default" }
    },
    "simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "default",
        "image": "latest"
      },
      "env": {
        "EXPO_IOS_VERBOSE_POD_INSTALL": "1",
        "EXPO_DEBUG": "1"
      }
    }
  },
  "submit": { "production": {} }
}
