{
  "name": "padelbrain",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "typecheck": "tsc --noEmit",
    "lint": "echo \"ESLint no configurado aún\" && exit 0",
    "format": "echo \"Prettier no configurado aún\" && exit 0",
    "test": "echo \"Jest no configurado aún\" && exit 0",
    "validate": "npm run typecheck",
    "clean": "powershell -NoProfile -Command \"if (Test-Path node_modules) { Remove-Item node_modules -Recurse -Force }; if (Test-Path ios) { Remove-Item ios -Recurse -Force }; if (Test-Path android) { Remove-Item android -Recurse -Force }; npm install\""
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@react-native-async-storage/async-storage": "~1.21.0",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-voice/voice": "^3.1.5",
    "expo": "~54.0.13",
    "expo-dev-client": "~5.0.6",
    "expo-print": "^15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-get-random-values": "~1.8.0",
    "react-native-web": "~0.19.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@types/react": "~18.2.45",
    "babel-plugin-module-resolver": "^5.0.0",
    "typescript": "^5.3.3"
  },
  "private": true
}
