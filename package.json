{
  "name": "padelbrain",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "typecheck": "tsc --noEmit",
    "lint": "echo \"ESLint no configurado aún\" && exit 0",
    "format": "echo \"Prettier no configurado aún\" && exit 0",
    "test": "echo \"Jest no configurado aún\" && exit 0",
    "validate": "npm run typecheck",
    "clean": "powershell -NoProfile -Command \"if (Test-Path node_modules) { Remove-Item node_modules -Recurse -Force }; if (Test-Path ios) { Remove-Item ios -Recurse -Force }; if (Test-Path android) { Remove-Item android -Recurse -Force }; npm install\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/slider": "^4.5.7",
    "@react-native-voice/voice": "^3.1.5",
    "expo": "~54.0.13",
    "expo-dev-client": "~6.0.15",
    "expo-print": "^15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-get-random-values": "~1.11.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-web": "^0.21.0",
    "uuid": "^9.0.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "@types/uuid": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "patch-package": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "private": true
}
