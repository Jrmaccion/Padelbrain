{
  "name": "padelbrain",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "homepage": ".",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "typecheck": "tsc --noEmit",
    "lint": "echo \"ESLint no configurado aún\" && exit 0",
    "format": "echo \"Prettier no configurado aún\" && exit 0",
    "test": "echo \"Jest no configurado aún\" && exit 0",
    "validate": "npm run typecheck",
    "clean": "powershell -NoProfile -Command \"if (Test-Path node_modules) { Remove-Item node_modules -Recurse -Force }; if (Test-Path ios) { Remove-Item ios -Recurse -Force }; if (Test-Path android) { Remove-Item android -Recurse -Force }; npm install\"",
    "postinstall": "patch-package",
    "build:web": "expo export:web",
    "build:production": "NODE_ENV=production expo export:web",
    "build:electron": "npm run build:web && electron-builder",
    "build:electron:win": "npm run build:web && electron-builder --win",
    "build:electron:mac": "npm run build:web && electron-builder --mac",
    "build:electron:linux": "npm run build:web && electron-builder --linux",
    "electron": "electron .",
    "electron:dev": "NODE_ENV=development electron .",
    "eas:preview:ios": "eas build --platform ios --profile preview",
    "eas:preview:android": "eas build --platform android --profile preview",
    "eas:production:ios": "eas build --platform ios --profile production",
    "eas:production:android": "eas build --platform android --profile production",
    "eas:production:all": "eas build --platform all --profile production",
    "eas:submit:ios": "eas submit --platform ios --profile production",
    "eas:submit:android": "eas submit --platform android --profile production"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/slider": "5.0.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "^54.0.13",
    "expo-build-properties": "^1.0.9",
    "expo-dev-client": "~6.0.15",
    "expo-print": "^15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-speech": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.12",
    "patch-package": "^8.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-get-random-values": "~1.11.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-web": "^0.21.0",
    "uuid": "^9.0.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "@types/uuid": "^10.0.0",
    "electron": "^33.4.1",
    "electron-builder": "^26.1.0",
    "typescript": "^5.3.3"
  },
  "build": {
    "extends": "./electron-builder.json"
  }
}
